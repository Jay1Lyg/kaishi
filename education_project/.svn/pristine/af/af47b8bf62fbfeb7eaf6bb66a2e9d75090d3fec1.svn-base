*{
    margin: 0;
    padding: 0;
}
header{
    margin-top: 20px;
}
.ul2{
    width: 100%;
    margin: 0 auto;
}
.ul2 li{
    text-align: center;
}
.ul2 li a{
    color: #333333;
    font-family: 微软雅黑;

}
.tab{
    background: white;
    border:1px solid #333333;
    width: 120%;
    display: none;

}
.tab li{
    list-style: none;
    border-bottom: 1px solid #cccccc;
    line-height: 50px;
    font-size: 16px;
    cursor: pointer;
}
.tab li a{
    color: #333333;
}

.top_logo{
    width: 100%;
    height: 134px;
    margin: 0 auto;
    text-align: center;
}
.top_logo img{
    width: 221px;
    margin: 0 auto;
}