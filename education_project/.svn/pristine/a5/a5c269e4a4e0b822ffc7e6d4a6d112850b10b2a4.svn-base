*{
    margin: 0;
    padding: 0;
}
header{
    margin-top: 20px;
}
.ul2{
    width: 100%;
    margin: 0 auto;
}
.ul2 li{
    text-align: center;
}
.ul2 li a{
    color: #333333;
    font-family: 微软雅黑;

}
.top_logo{
    width: 100%;
    height: 134px;
    margin: 0 auto;
    text-align: center;
}
.top_logo img{
    width: 221px;
    margin: 0 auto;
}

/*脚步*/
footer{
    background: #c3c3c3;
    width: 100%;
    height: 144px;
}
footer{
    padding: 40px 0;
}

footer div .ul3 li{
    list-style: none;
    text-align: center;

}
footer div .ul3 li img{
    width: 45px;
}
footer div .ul4 li{
    list-style: none;
    text-align: center;
    font-family: 微软雅黑;
    font-size: 12px;
    color: #6b6b6b;
}